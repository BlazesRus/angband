  <!--Use Integrated version of the code if GitProject of ZLib not found in parent of root-->
  <Choose>
    <When Condition="Exists('$(ParentOfRoot)\zlib-win64\zlib.h') And (Exists('$(ParentOfRoot)zlib-win64\out\build\$(Platform)-$(Configuration)\zlib.lib') Or Exists('$(ParentOfRoot)zlib-win64\$(Platform)\$(Configuration)\zlib.lib'))">
      <PropertyGroup>
        <ZLibLocType>X64</ZLibLocType>
        <ZLibDir>$(ParentOfRoot)zlib-win64\</ZLibDir>
        <!--Open Visual Studio 2022 and build via nmake-->
        <ZLibLibrary Condition="$(ZLibLibrary)=='' And Exists('$(ParentOfRoot)\zlib\out\build\$(Platform)-$(Configuration)\zlib.lib')">$(ZLibDir)$(Platform)\$(Configuration)\zlib.lib</ZLibLibrary>
        <!--Or just use Visual Studio project in Contribution/vs16 folder-->
        <ZLibLibrary Condition="$(ZLibLibrary)=='' And Exists('$(ZLibDir)$(Platform)\$(Configuration)\zlib.lib')">$(ZLibDir)$(Platform)\$(Configuration)\zlib.lib</ZLibLibrary>
        <ZLibLibrary Condition="$(ZLibLibrary)==''">$(ZLibDir)$(Platform)\$(Configuration)\zlib.lib</ZLibLibrary>
      </PropertyGroup>
    </When>
    <When Condition="Exists('$(ParentOfRoot)\zlib\out\build\x64-Release\zlib.lib') And Exists('$(ParentOfRoot)zlib\out\build\$(Platform)-$(Configuration)\zlib.lib')">
      <PropertyGroup>
        <ZLibLocType>Normal</ZLibLocType>
        <ZLibDir>$(ParentOfRoot)zlib\</ZLibDir>
        <ZLibLibrary>$(ZLibDir)out\build\$(Platform)-$(Configuration)\zlib.lib</ZLibLibrary>
      </PropertyGroup>
    </When>
    <Otherwise>
      <PropertyGroup>
        <ZLibLocType>Integrated</ZLibLocType>
        <ZLibDir>$(SPRoot)\win\IntegratedZLib\</ZLibDir>
        <ZLibLibrary>$(SPRoot)\win\lib\$(Platform)\zlib.lib</ZLibLibrary>
      </PropertyGroup>
    </Otherwise>
  </Choose>
